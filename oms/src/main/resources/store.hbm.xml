<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.sapient.oms.entity.Store" table = "stores">
      <meta attribute = "class-description">
         This class contains the product blueprint. 
      </meta>
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "shopName" column = "shop_name" type = "string"/>
      <property name = "contactNumber" column = "contact_number" type = "int"/>
      <property name = "emailId" column = "email_id" type = "string"/>
    
   </class>
</hibernate-mapping>